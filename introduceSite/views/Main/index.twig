{% extends '../layout.twig' %}



{% block content %}



    <div class="profile-card">
    <div class="profile-info">

            <div>
                <div class="view overlay hm-white-slight profile-image-container">
                    <img src="public/myResource/myImage/my_photo.jpg" class="img-flex profile-photo">
                    <a href="https://stackexchange.com/users/9552681" style="bottom: 0;position: absolute;">
                        <img src="https://stackexchange.com/users/flair/9552681.png?theme=clean"
                             alt="profile for Аксенов Владимир on Stack Exchange, a network of free, community-driven Q&amp;A sites"
                             title="profile for Аксенов Владимир on Stack Exchange, a network of free, community-driven Q&amp;A sites">
                    </a>
                </div>

                <div class="card-body text-center">
                    <h3 class="card-title"><strong>{{ res.myFullName }}</strong></h3>
                    <h4>{{ res.developer }}</h4>

                    <p class="card-text" style="font-size: 1.2em">{{ res.myDescribe }}
                    </p>
                </div>
                <div class="media mb-1 skill">
                    <div class="media-body">
                        <div class="card current-job">
                            <img src="public/myResource/myImage/job-logo.png" style="border-radius: 50%;width: 60px">
                            На данный момент Android разработчик в <a href="https://touchin.ru/">TouchInstinct</a>
                        </div>
                        <h4 class="">{{ res.technologyStack }}</h4>
                        <p>
                            <span class="badge grey badge-element" style="font-size: 100%">Git</span>
                            <span class="badge blue badge-element" style="font-size: 100%">SQLite/ROOM/Anko</span>
                            <span class="badge red badge-element" style="font-size: 100%">RxJava</span>
                            <span class="badge green badge-element" style="font-size: 100%">Dagger 2</span>
                            <span class="badge orange badge-element" style="font-size: 100%">Firebase</span>
                            <br>
                        </p>
                    </div>
                </div>
            </div>
            <div class="skills-list skill-description">
                <div class="media mb-1 skill">
                    <div class="media-body">
                        <h4 class="">Java</h4>
                        <ul class="star-rating" style="padding: 0px">
                            <li><i class="fa fa-star amber-text"></i></li>
                            <li><i class="fa fa-star amber-text"></i></li>
                            <li><i class="fa fa-star amber-text"></i></li>
                            <li><i class="fa fa-star amber-text"></i></li>
                            <li><i class="fa fa-star"></i></li>
                        </ul>
                        <p>Проекты:
                            {% for project in res.javaDescribeStack %}
                                <a
                                href="{{ project.link }}">{{ project.name }}</a>{% if project != res.javaDescribeStack|last %},&nbsp;{% endif %}
                            {% endfor %}
                        </p>
                    </div>
                </div>


